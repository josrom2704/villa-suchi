{
  "buildCommand": "npx vite build --config vite.config.prod.ts",
  "outputDirectory": "dist/spa",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
